def solution(board, moves):
    stack = []
    cnt = 0
    for i in moves:
        for j in range(len(board[0])):
            if board[j][i-1] != 0:
                stack.append(board[j][i-1])
                board[j][i-1] = 0
                break
        if len(stack) >= 2 and stack[-1] == stack[-2]:
            stack.pop()
            stack.pop()
            cnt += 2
    return cnt