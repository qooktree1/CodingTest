def solution(progresses, speeds):
    cnt = 0
    answer = []
    while True:

        for i in range(len(progresses)):
            progresses[i] += speeds[i]

        if progresses[0] >= 100:

            while progresses[0] >= 100:
                progresses.pop(0)
                speeds.pop(0)
                cnt += 1
                if len(progresses) == 0:
                    answer.append(cnt)
                    return answer
            answer.append(cnt)
            cnt = 0